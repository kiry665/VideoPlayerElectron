{
  "name": "video-player-electron",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "kiry665",
  "license": "ISC",
  "devDependencies": {
    "electron": "^30.0.6"
  },
  "dependencies": {
    "electron-prompt": "^1.7.0",
    "electron-settings": "^4.0.4",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "video.js": "^8.12.0",
    "videojs-youtube": "^3.0.1"
  },
  "build": {
    "appId": "com.example.video-player-electron",
    "productName": "video-player-electron",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html",
      "index.js",
      "index.css",
      "SocketServer.js",
      "SocketClient.js",
      "index2.html",
      "index2.js",
      "node_modules/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
